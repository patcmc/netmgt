# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-*

# Ignore logs and temp files.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore PID files, but keep the directory.
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/
!/tmp/storage/.keep

# Ignore assets and builds.
/public/assets
/public/packs
/public/packs-test
/public/system
/public/uploads

/app/assets/builds/*
!/app/assets/builds/.keep

# Ignore master key and credentials.
/config/master.key
/config/credentials.yml.enc

# Ignore Node modules and Yarn files.
node_modules/
/yarn-error.log

# Ignore environment files.
.env
.env.*

# Ignore Byebug command history.
/.byebug_history

# Ignore system files.
.DS_Store
Thumbs.db

# Ignore editor and IDE files.
.idea/
.vscode/
*.swp
*~

# Ignore Docker files.
/dump.rdb
/docker-compose.override.yml

# Ignore temporary and cache files.
/*.rbc
/.sass-cache
/capybara-*.html
/coverage/
/spec/tmp
**.orig

# Ignore other unnecessary files.
/vendor/bundle